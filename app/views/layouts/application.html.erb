<!DOCTYPE html>
<html>
<head>
  <title>BucketList</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
  <script type="text/javascript"
      src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAJvXxfl8SI3THf3FnHoxFXaif50SxpHKk">
  </script>
</head>
<body>

<div id="flash">
  <span class="notice"><%= notice %></span>
  <span class="alert"><%= alert %></span>
</div>


<header>
  <div id="sessions">
    <%= link_to "Sign up", new_traveller_registration_path %>
    <%= link_to "Sign in", new_traveller_session_path %>
    <%= link_to "Sign out", destroy_traveller_session_path, method: :DELETE %>
  </div>
  <h3 class="page-selection"><%= link_to "Bucket Lists", profiles_path %></h3>
</header>

  <main>

    <%= yield %>
  </main>

</body>
</html>
